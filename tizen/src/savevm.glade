<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="savevm_window">
    <property name="width_request">340</property>
    <property name="height_request">90</property>
    <property name="resizable">False</property>
    <property name="window_position">center-always</property>
    <signal name="destroy_event" handler="on_savevm_window_destroy_event"/>
    <child>
      <object class="GtkFixed" id="fixed1">
        <property name="width_request">340</property>
        <property name="height_request">90</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="savevm_label">
            <property name="width_request">300</property>
            <property name="height_request">34</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">Saving Emulator State...Please wait</property>
          </object>
          <packing>
            <property name="x">10</property>
            <property name="y">5</property>
          </packing>
        </child>
        <child>
          <object class="GtkProgressBar" id="savevm_progress">
            <property name="width_request">311</property>
            <property name="height_request">32</property>
            <property name="visible">True</property>
            <property name="text" translatable="yes">Saving Emulator State</property>
            <property name="ellipsize">middle</property>
          </object>
          <packing>
            <property name="x">16</property>
            <property name="y">38</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
