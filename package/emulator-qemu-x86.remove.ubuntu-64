#!/bin/sh

TIZEN_SDK_INSTALL_PATH=`echo $INSTALLED_PATH`
if [ -z $TIZEN_SDK_INSTALL_PATH ]
then
#   echo "There is no TIZEN_SDK_PATH ENV" >> /tmp/emulator.log
   exit 2;
fi

TMP_REMOVE_FILE=tmp_inst.sh
echo "update-rc.d -f tizen-kvm remove" >> $TMP_REMOVE_FILE
echo "rm -f /etc/init.d/tizen-kvm" >> $TMP_REMOVE_FILE
echo "rm -f /lib/udev/rules.d/45-tizen-kvm.rules" >> $TMP_REMOVE_FILE

chmod +x $TMP_REMOVE_FILE
gksudo ./$TMP_REMOVE_FILE

rm $TMP_REMOVE_FILE
