#!/bin/sh

TIZEN_SDK_INSTALL_PATH=`echo $INSTALLED_PATH`
if [ -z $TIZEN_SDK_INSTALL_PATH ]
then
#   echo "There is no TIZEN_SDK_PATH ENV" >> /tmp/emulator.log
   exit 2;
fi

gksudo cp ${TIZEN_SDK_INSTALL_PATH}/tools/emulator/etc/tizen_kvm /etc/init.d/
gksudo cp ${TIZEN_SDK_INSTALL_PATH}/tools/emulator/etc/45-tizen-kvm.rules /lib/udev/rules.d/
gksudo update-rc.d tizen-kvm defaults

